        "**/.DS_Store": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.svn": true,
        "**/CVS": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
